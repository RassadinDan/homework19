	ViewData["Title"] = "Контакты";
}


<head>
	
</head>

<body>
	<h1>
		@ViewData["Title"]
	</h1>
	<h2>
		Некие контакты
	</h2>
	<table border="1">
		<tr>
			<th>
				ID
			</th>
			<th>
				Фамилия
			</th>
			<th>
				Имя
			</th>
			<th>
				Отчество
			</th>
